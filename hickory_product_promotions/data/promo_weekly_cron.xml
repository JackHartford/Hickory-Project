<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>


        <record forcecreate="True" id="promotion_cron" model="ir.cron">
            <field name="name">Promo Products To Customer</field>
            <field eval="False" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">weeks</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall" />
            <field ref="model_res_partner" name="model_id"/>
            <field name="nextcall" eval="(DateTime.now() + timedelta(days=7)).strftime('%Y-%m-%d %H:%M:%S')" />
            <field name="state">code</field>
            <field name="code">model.send_promo_url()</field>
        </record>



    </data>
</odoo>
