<?xml version="1.0" encoding="utf-8"?>
<odoo>
   <template id="assets_backend" name="product_variant_extension assets backend" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/product_variant_extension/static/src/js/fields.js"></script>
        </xpath>

        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/less" href="/product_variant_extension/static/src/less/library.less"/>
        </xpath>
    </template>

</odoo>
